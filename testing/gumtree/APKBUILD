pkgname=gumtree
pkgver=2.1.0
pkgrel=0
pkgdesc="GumTreeDiff"
url="https://github.com/yijunyu/gumtree"
arch="all"
license="BSD"
depends="openjdk8"
depends_dev=""
install=""
source="gumtree.zip::http://docker.for.mac.localhost:8080/gumtree.zip"
#subpackages="$pkgname-dev"
builddir="$srcdir"
options="!check !fhs"
build() {
	echo prebuilt
}

package() {
        echo "$(pwd)"
        echo "$pkgdir"
        mkdir -p "$pkgdir"/usr
        cp -r gumtree-*-2.1.0-SNAPSHOT/* "$pkgdir"/usr/
}
