#~/bin/setup.sh
sudo apk update
sudo apk add --allow-untrusted /target/testing/x86_64/antlr-2.7.7-r0.apk
sudo apk add --allow-untrusted /target/testing/x86_64/antlr-dev-2.7.7-r0.apk
sudo apk add --allow-untrusted /target/testing/x86_64/antlr4-4.7-r0.apk
sudo apk add --allow-untrusted /target/testing/x86_64/antlr4-dev-4.7-r0.apk
sudo apk add --allow-untrusted /target/testing/x86_64/srcml-0.9.5-r0.apk
sudo apk add --allow-untrusted /target/testing/x86_64/srcml-dev-0.9.5-r0.apk
sudo apk add --allow-untrusted /target/testing/x86_64/srcSlice-1.0-r0.apk
sudo apk add --allow-untrusted /target/testing/x86_64/srcSlice-dev-1.0-r0.apk
sudo apk add --allow-untrusted /target/testing/x86_64/flatbuffers-1.10.0-r0.apk
sudo apk add --allow-untrusted /target/testing/x86_64/flatbuffers-dev-1.10.0-r0.apk
sudo apk add --allow-untrusted /target/testing/x86_64/protobuf-3.6.1-r1.apk
sudo apk add coreutils openjdk8 perl jq
sudo abuild-keygen -a -i -n
../p
